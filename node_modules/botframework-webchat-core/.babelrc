{
  "env": {
    "test": {
      "plugins": [
        "babel-plugin-istanbul"
      ]
    }
  },
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-runtime",
    "babel-plugin-version-transform"
  ],
  "presets": [
    "@babel/preset-typescript",
    ["@babel/preset-env", {
      "forceAllTransforms": true,
      "modules": "commonjs"
    }]
  ]
}
